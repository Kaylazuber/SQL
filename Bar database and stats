--create a grocery store data base. 

CREATE TABLE Bar (id INTEGER PRIMARY KEY, liquor TEXT, quantity INTEGER, brand TEXT, price INTEGER, color TEXT);

INSERT INTO bar VALUES (1, "Vodka", 85, "Titos", 5, "clear");
